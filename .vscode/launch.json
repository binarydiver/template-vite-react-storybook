  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "Run Storybook",
      "command": "pnpm run storybook",
      "internalConsoleOptions": "openOnFirstSessionStart",
      "serverReadyAction": {
        "pattern": "https?:.*([0-9]{4})",
        "uriFormat": "http://localhost:%s",
        "action": "debugWithEdge"
      }
    }
  ]
}
